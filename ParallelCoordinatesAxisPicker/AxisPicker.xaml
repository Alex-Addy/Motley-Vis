<Window x:Class="ParallelCoordinatesAxisPicker.AxisPickerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Axis Picker" Height="180" Width="408">
    <Grid>
        <Button Content="Swap" HorizontalAlignment="Left" Margin="315,119,0,0" VerticalAlignment="Top" Width="75" Click="Swap_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="235,119,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <ComboBox HorizontalAlignment="Left" Margin="24,61,0,0" VerticalAlignment="Top" Width="120" x:Name="FirstBox"/>
        <ComboBox HorizontalAlignment="Left" Margin="149,61,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="1.542,0.5" x:Name="SecondBox"/>
        <Label Content="First Axis" HorizontalAlignment="Left" Margin="24,30,0,0" VerticalAlignment="Top"/>
        <Label Content="Second Axis" HorizontalAlignment="Left" Margin="149,30,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
